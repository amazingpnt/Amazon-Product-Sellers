{
  "manifest_version": 3,
  "name":"yt",
  "version":"1.0.0",
  "description":"works",
  "permissions":["activeTab"],
  "host_permissions":[
    "*://www.youtube.com/*"
  ],
  "content_scripts":[
    {
      "matches": [
        "*://www.youtube.com/watch?v=*"
      ],
      "js":["content.js"],
      "run_at":"document_idle"
    }
  ],
  "action":{
    "default_popup":"popup/popup.html"
  }
}
